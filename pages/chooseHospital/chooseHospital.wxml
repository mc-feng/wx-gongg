<!--pages/chooseHospital/chooseHospital.wxml-->
<view class='image-header'>
  <image src='../../images/homeimages/search.png'></image>
  <view class='image-font'>查询到以下信息</view>
</view>
<view class='card-title'>可绑定绑定就诊卡</view>
<block wx:if="{{addbind.length == 0}}">
 <view class='may-bind'>
    暂无可绑定的就诊市区
  </view>
</block>
<block wx:else>
  <block wx:for="{{addbind}}" wx:key="{{index}}">
    <view class='may-bind' bindtap='addToCard' data-hosName="{{item}}">
      上海市公卫中心（{{item}}）
    </view>
  </block>
</block>
<view class='card-title'>已绑定就诊卡</view>
<block wx:if="{{alreadybind.length == 0}}">
 <view class='already-bind'>
    暂无
  </view>
</block>
<block wx:else>
  <block wx:for="{{alreadybind}}" wx:key="{{index}}">
    <view class='already-bind'>
      上海市公卫中心（{{item}}）
    </view>
  </block>
</block>