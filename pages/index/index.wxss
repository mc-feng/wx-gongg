/**index.wxss**/
.main-container{
  background-color:#aaa
}
.index-header{
  height: 138rpx;
  background-color: rgba(24,179,151,1);
  position: relative
}
.index-header-color{
  left: 40rpx;
  top: 62rpx;
  width: 400rpx;
  height: 52rpx;
  line-height: 52rpx;
  color: rgba(255, 255, 255, 1);
  font-size: 36rpx;
  text-align: left;
  font-family: Arial;
  position: absolute;
}
swiper {
    height: 421.5rpx;
}
swiper-item image {
    width: 100%;
    height: 100%;
}
.swiper-container{
  position: relative;
}
.swiper-container .swiper{
  height: 300rpx;
}

.swiper-container .swiper .img{
  width: 100%;
  height: 100%;
}
.index-tips{
  background: #fff;
  height:80rpx;
  padding: 10rpx 0 8rpx 0;
  display: flex;
  flex-flow: row nowrap;
}
.index-tips image{
  height: 80rpx;
  width: 170rpx;
}
.index-tips-color{
  padding:22rpx 0 18rpx 0; 
  width: 566rpx;
  height: 40rpx;
  line-height: 40rpx;
  color: rgba(136, 136, 136, 1);
  font-size: 28rpx;
  text-align: left;
  font-family: Arial;
}
.hometips-nav{
  background-color: #fff;
  height: 200rpx;
  border: 2rpx solid rgba(187,187,187,1);
  border-right: none;
  border-bottom: none;
  display: flex;
  flex-flow: row wrap;
}
.hometips-nav navigator{
  border: 2rpx solid rgba(187,187,187,1);
  border-left: none;
  border-top: none;
  height: 46rpx;
  width: 228rpx;
  padding:32rpx 0rpx 22rpx 144rpx;
  line-height: 46rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 32rpx;
  text-align: left;
  font-family: Arial;
}
.main-content{
  margin-top: 14rpx;
  background-color: #fff
}
.main-header{
  height: 76rpx;
  padding: 8rpx 0 8rpx 0;
  border-bottom: 2rpx solid rgba(187,187,187,1);
  display: flex;
  flex-flow: row nowrap;
}
.main-header-left{
  height: 78rpx;
  width: 10rpx;
  margin-left: 10rpx;
  background: #18B397 100%
}
.main-header-right{
  height: 62rpx;
  padding: 16rpx;
  margin-left: 38rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 32rpx;
  text-align: center;
  font-family: Arial;
}
.main-nav{
  padding: 7px 32px 16px 29px;
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
}
.main-nav navigator{
  margin-top: 20rpx;
  width: 127rpx;
  display: flex;
  flex-flow: column wrap;
}
.mainnav-box{
  width: 108rpx;
  height: 108rpx;
  margin-left: 6rpx;
  border-radius: 50%;
  background-color: rgba(24, 179, 151, 1);
  color: rgba(16, 16, 16, 1);
  font-size: 14px;
  text-align: center;
  font-family: Arial;
  border: 1px solid rgba(255, 255, 255, 0);
}
.mainnav-color{
  margin-top: 10rpx;
  height: 46rpx;
  line-height: 40rpx;
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  text-align: center;
  font-family: Arial;
}
.sub-main .main-header{
  margin-top: 14rpx;
  background-color: #fff;
  border-bottom: none;
}
.sub-main .hometips-nav navigator{
  color: rgba(16, 16, 16, 1);
  font-size: 28rpx;
  text-align: left;
  font-family: Arial;
}