<view class='bookordermain-wrap'>
  <view class='bookordermain'>
    <!-- 显示就诊状态 -->
     <view class='relation {{relations =="待就诊"?"":"gray"}}'>{{relations}}
            <view class='trigon'></view>
      </view>
      <!-- 医院信息 -->
     <view class='bookordermain-item'>
         <view class='bookordermain-title'>所属医院：</view>
         <view class='bookordermain-content'>{{data.trans.hospital}}</view>
     </view>
     <view class='bookordermain-item'>
         <view class='bookordermain-title'>所属科室：</view>
         <view class='bookordermain-content'>{{data.trans.office}}</view>
     </view>
     <view class='bookordermain-item'>
         <view class='bookordermain-title'>医生名称：</view>
         <view class='bookordermain-content'>{{data.trans.docName}}</view>
     </view>
     <view class='bookordermain-item'>
         <view class='bookordermain-title'>挂号费用：</view>
         <view class='bookordermain-content'>{{data.price}}元（医院取号支付）</view>
     </view>
     <view class='bookordermain-item'>
         <view class='bookordermain-title'>预约时间：</view>
         <view class='bookordermain-content black'>{{data.now}}    {{data.time}}</view>
     </view>
     <!-- 分割线 -->
     <view class='Cutting-line'>
       <view class='line'></view>
       <view class='circle'></view>
       <view class='circle1'></view>
     </view>
     <!-- 患者信息 -->
    <view class='bookordermain-item'>
      <view class='bookordermain-title'>就诊人： </view>
      <view class='bookordermain-content black'>{{patientMessage.openUserName}}</view>
    </view>
    <view class='bookordermain-item'>
      <view class='bookordermain-title'>就诊身份证：</view>
      <view class='bookordermain-content'>{{patientMessage.openIDCard}}</view>
    </view>
    <view class='bookordermain-item'>
      <view class='bookordermain-title'>就诊手机号：</view>
      <view class='bookordermain-content'>{{patientMessage.openTel}}</view>
    </view>
    <!-- 分割线 -->
     <view class='Cutting-line small-height'>
       <view class='line'></view>
       <view class='circle'></view>
       <view class='circle1'></view>
     </view>
     <!-- 预约号 -->
     <view class='yuding-number'>
       <view class='number-font'>预约号码</view>
       <view class='number-shuzi'>{{result}}</view>
     </view>
  </view>
</view>
<!-- 按钮 -->
<view class='newbuttun {{relations =="待就诊"?"":"disno"}}' bindtap='cancel'>
  取消预约
</view>