/* 医生样式 */
page{
  background: #F5F7F9;
}
.doctor{
  height: 44rpx;
  line-height: 44rpx;
  margin-top: 2rpx;
  background-color: #ffffff;
  display: flex;
  flex-flow: row nowrap;
  padding: 46rpx 0 50rpx 132rpx;
}
.doctor-name{
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #232323;
}
.doctor-position{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #A3A3A3;
  margin-left: 24rpx;
}
.doctor-collect{
  position: relative;
  padding-left: 24rpx;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #232323;
  margin-left: 278rpx
}
.doctor-collect image{
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  left: -7rpx;
  top: 10rpx;
}
/* 介绍 */
.introduce{
  margin-top:2rpx; 
  padding: 20rpx 28rpx 30rpx 28rpx;
  background: #ffffff;
}
.introduce-header{
  position: relative;
  height: 42rpx;
  line-height: 42rpx;
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
  color: #232323;
}
.introduce-header image{
  height: 16rpx;
  width: 28rpx;
  position: absolute;
  top:24rpx;
  right: 0
}
.introduce-content{
  margin-top: 10rpx;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #666666;
  text-align: justify;
}
.content{
  margin-top: 20rpx;
}
.yuding-riqi{
  position: relative;
  height: 34rpx;
  padding: 26rpx 0 20rpx 74rpx;
  line-height: 34rpx;
  font-family: PingFangSC-Medium;
  font-size: 12px;
  color: #232323;
  background: #ffffff;
}
.yuding-riqi image{
  width: 30rpx;
  height: 26rpx;
  position: absolute;
  left: 34rpx;
  top: 30rpx
}
.yuding-details{
  margin-top: 2rpx;
  height: 40rpx;
  line-height: 40rpx;
  padding: 26rpx 0 24rpx;
  background-color: white;
  display: flex;
  flex-flow: row nowrap;
  font-size: 28rpx;
  color: #A3A3A3;
  font-family: PingFangSC-Regular;
}
.yuding-state{
  margin-left: 28rpx;
  width: 60rpx;
}
.yuding-time{
  width: 203rpx;
  margin-left: 20rpx;
}
.yuding-price{
  width: 96rpx;
  margin-left: 306rpx;
}
.other .yuding-state{
  color: #0D86EB;
}
.other .yuding-time{
  color: #111111;
}
.other .yuding-price{
  color: #F58F08;
}