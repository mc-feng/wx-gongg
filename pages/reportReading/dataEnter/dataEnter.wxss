/* pages/reportReading/dataEnter/dataEnter.wxss */
page{
  background: #F6F9FB;
  padding-bottom: 96rpx;
}
.tips{
  height: 56rpx;
  background: #CFE6FF;
  text-align: center;
  line-height: 56rpx;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #0D86EB;
  letter-spacing: -0.4rpx;
}
.complete-tips{
  padding: 10rpx 58rpx 12rpx 24rpx;
  border-bottom: 2rpx solid#DCDCDC;
  background: #ffffff;
}
.complete-font{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #666666;
  line-height: 34rpx;
}
.tip-click{
  color: #0D86EB;
}
.examine-data{
  height: 80rpx;
  padding: 0 24rpx 0 24rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
  background: #fff;
}
.examine-font{
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #232323;
  letter-spacing: 0;
}
.examine-number{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #0D86EB;
  letter-spacing: 0;
}
.report-title{
  height: 70rpx;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
}
.title-icon{
  height: 28rpx;
  width: 4rpx;
  background: #0D86EB;
  margin-left: 26rpx;
}
.title-font{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #000000;
  letter-spacing: -0.48rpx;
  text-align: center;
  line-height: 34rpx;
  margin-left: 6rpx;
}
.xue-content{
  padding: 30rpx 20rpx 34rpx 26rpx;
  background: #ffffff;
}
.xue-title{
  display: flex;
  flex-flow: row nowrap;
}
.jieguo{
  margin-left: 228rpx
}
.danwei{
  margin-left: 228rpx
}
.details{
  margin-left: 57rpx
}
.xue-title view{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #A3A3A3;
  letter-spacing: -0.48rpx;
  text-align: center;
}
.content-result{
  margin:24rpx 0 28rpx 0;
  display: flex;
  flex-flow: row nowrap;
}
.content-xiangmu{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #000000;
  letter-spacing: -0.48rpx;
}
.content-jieguo{
  margin-left: 96rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #FF4B00;
  position: relative;
}
.content-danwei{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #A3A3A3;
  letter-spacing: 0;
  margin-left: 244rpx;
}
.content-details{
  width: 40rpx;
  height: 40rpx;
  margin-left: 24rpx
}
.content-position{
  position: absolute;
  width: 324rpx;
  height: 2rpx;
  left: -56rpx;
  bottom:0rpx;
  background: #D8D8D8;
}
.content-tips{
  height: 50rpx;
  text-align: center;
}
.submit{
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
  background: #ffffff;
  margin-top:10rpx;
}
.submit-font{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #232323;
  letter-spacing: 0;
  text-align: center;
  margin-top: 30rpx;
}
.submit-buttun{
  width: 160rpx;
  height: 70rpx;
  border: 2rpx solid #0D86EB;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #0D86EB;
  letter-spacing: 0;
  text-align: center;
  line-height: 70rpx;
  margin: 20rpx 0rpx 46rpx 0rpx;
}
.examine-image{
  height: 120rpx;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  padding: 0rpx 34rpx 0 38rpx;
  justify-content: space-between
}
.examine-image-left{
  width: 120rpx;
  height: 80rpx;
  background: #D8D8D8;
}
.examine-image-right{
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #0D86EB;
  letter-spacing: 0;
  text-align: center;
}
.save-submit{
  width: 100%;
  position: fixed;
  bottom: 0rpx;
  height: 96rpx;
  background:  #0D86EB;
  text-align: center;
  line-height: 96rpx;
  color: #ffffff
}