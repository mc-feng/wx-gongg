   position: absolute;
   bottom: 60rpx
}
/* 下拉框 */
.select_box{
  background: #fff;
  width: 80%;
  margin: 30rpx auto;
  position: relative;
}
.select{
  box-sizing: border-box;
  width: 100%;
  display: flex;
  align-items: center;
  margin: 20rpx;
}
.select_text{
  font-size: 28rpx;
  flex: 1;
}
.select_img{
  width: 24rpx;
  height: 16rpx;
  display: block;
  transition:transform 0.3s;
}
.select_img_rotate{
  transform:rotate(180deg); 
}
.option_box{
  position: absolute;
  top: 70rpx;
  width: 464rpx;
  box-sizing: border-box;
  height: 0;
  overflow-y: auto;
  background: #fff;
  transition: height 0.3s;
  z-index: 10;
  left: -2rpx
}
.option{
  display: block;
  line-height: 60rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #efefef;
  padding: 10rpx;
}