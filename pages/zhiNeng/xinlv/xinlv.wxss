/* pages/zhiNeng/tiwen/tiwen.wxss */
.canvas {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
ff-canvas{
  height: 760rpx;
  width: 740rpx;
}
.selectdate{
  width: 100%
}
.selectDate{
  width: 100%;
  height:96rpx;
  background:rgba(13,134,235,1);
  text-align: center;
  font-size:32rpx;
  color:rgba(255,255,255,1);
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.found{
  position: absolute;
  height:44rpx;
  font-size:32rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:44rpx;
  right: 40rpx;
}
.tips-warp{
  margin-top: 20rpx;
  margin-left: 28rpx;
}
.item-warp{
  display: flex; 
  margin-bottom: 36rpx;
  justify-content: space-between
}
.item-iamge{
  width: 40rpx;
  height: 32rpx;
  margin-right: 12rpx;
}
.item-left{
  height:56rpx;
  font-size:40rpx;
  font-weight:500;
  line-height:56rpx;
}
.item-right{
  font-size:24rpx;
  font-weight:500;
  color:rgba(68,215,182,1);
  text-align: center;
  display: flex;
  width: 330rpx;
  justify-content: space-between
}
.item-min{
  margin-right: 28rpx;
}
.item-top{
  min-width: 100rpx
}
.item-botton{
  color:rgba(102,102,102,1);
  margin-top: 10rpx;
}
.blue{
  color:rgba(92,137,255,1);
}