/* pages/zhiNeng/tiwen/tiwen.wxss */
.canvas {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
ff-canvas{
  height: 760rpx;
  width: 740rpx;
}
.selectdate{
  width: 100%
}
.selectDate{
  width: 100%;
  height:96rpx;
  background:rgba(13,134,235,1);
  text-align: center;
  font-size:32rpx;
  color:rgba(255,255,255,1);
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.found{
  position: absolute;
  height:44rpx;
  font-size:32rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:44rpx;
  right: 40rpx;
}
.tips-warp{
  display: flex;
  justify-content: center;
  margin-top: 20rpx
}
.warp1{
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
}
.warp2{
  margin-left: 36rpx;
  height:34rpx;
  font-size:24rpx;
  font-weight:500;
  color:rgba(136,155,255,1);
  line-height:34rpx;
}
.tems{
  height:34rpx;
  font-size:24rpx;
  font-weight:500;
  color:rgba(136,155,255,1);
  line-height:34rpx;
}
.time{
  height:34rpx;
  font-size:24rpx;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height:34rpx;
  margin-top: 10rpx
}