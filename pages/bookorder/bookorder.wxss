/* 模态框 */
.commodity_screen {
 width: 100%;
 height: 100%;
 position: fixed;
 top: 0;
 left: 0;
 background: #000;
 opacity: 0.2;
 overflow: hidden;
 z-index: 1000;
 color: #fff;
}
 
.commodity_attr_box {
 width: 572rpx;
 position: fixed;
 top:278rpx;
 left:90rpx;
 z-index: 2000;
 height: 542rpx;
 background: #fff;
 display: flex;
 flex-flow: column nowrap;
 align-items: center;
 border-radius: 18rpx;
 
}
