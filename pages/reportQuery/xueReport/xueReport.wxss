/* pages/reportQuery/xueReport/xueReport.wxss */
page{
  background: #F6F9FB;
}
.report-title{
  height: 70rpx;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
}
.title-icon{
  height: 28rpx;
  width: 4rpx;
  background: #0D86EB;
  margin-left: 26rpx;
}
.title-font{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #000000;
  letter-spacing: -0.48rpx;
  text-align: center;
  line-height: 34rpx;
  margin-left: 6rpx;
}
.xue-content{
  padding: 30rpx 20rpx 34rpx 26rpx;
  background: #ffffff;
}
.xue-title{
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between
}
.xue-title view{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #A3A3A3;
  letter-spacing: -0.48rpx;
  text-align: center;
}
.xue-result{
  height: 40rpx;
  width:100%;
  margin-top: 20rpx;
  display: flex;
  flex-flow: row nowrap;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #000000;
  letter-spacing: -0.56rpx;
  text-align: center;
  position: relative
}
.xue-result view{
  position: absolute
}
.project{
 left: 0;
 top:0;
}
.number{
  left: 224rpx;
  top: 0rpx;
}
.unit{
  left: 438rpx;
  top: 0rpx;
}
.details{
  left: 668rpx;
  top:-8rpx;
}
.details image{
  width: 24rpx;
  height: 14rpx;
}
.tips-image{
  position: absolute;
  left: 270rpx;
  top:8rpx;
  width: 16rpx;
  height: 24rpx;
}
.view-content{
  background: #ffffff;
}
.view-image{
  margin-top: 30rpx;
  height: 116rpx;
  width: 100%;
  position: relative;
}
.view-rectangle{
  display: flex;
  flex-flow: row nowrap;
  position: absolute;
  top: 60rpx;
  padding: 0rpx 24rpx;
  left: 0rpx;
}
.rectangle-left-raduis{
  height: 24rpx;
  width: 24rpx;
  background: #4989F3;
  position: absolute;
  left: 0;
  top: 0;
  border-top-left-radius: 50%;
  border-bottom-left-radius: 50%
}
.rectangle-right-raduis{
  height: 24rpx;
  width: 24rpx;
  background: #FF7040;
  position: absolute;
  right: 0;
  top: 0;
  border-top-right-radius: 50%;
  border-bottom-right-radius: 50%
}
.rectangle-left{
  height: 24rpx;
  width: 206rpx;
  background: #4989F3
}
.rectangle-mid{
  height: 24rpx;
  width: 230rpx;
  background: #1DDE98
}
.rectangle-right{
  height: 24rpx;
  width: 206rpx;
  background: #FF7040 
}
.rectangle-bottom{
  display: flex;
  flex-flow: row nowrap;
  position: absolute;
  top: 84rpx;
  left: 0;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  letter-spacing: 0;
  text-align: center;
}
.rectangle-raduis{
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  border:4rpx solid #0D86EB;
  background: #ffffff;
  position: absolute;
  left: 40rpx;
  top: -6rpx
}
.raduis-font{
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #232323;
  letter-spacing: 0;
  text-align: center;
  position: absolute;
  left: -18rpx;
  top:-42rpx;
}
.rectangle-bottom view{
  position: absolute;
}
