/* pages/reportQuery/query/query.wxss */
page{
  background: #F5F7F9;
  display: flex;
  flex-flow: column nowrap;
  justify-content: center;
  align-items: center;
  padding-bottom: 80rpx;
}
.header{
  height: 96rpx;
  width: 100%;
  background:  #0D86EB;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center
}
.header-font{
  margin-left: 30rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-family: PingFangSC-Medium;
  font-size: 34rpx;
  color: #FFFFFF;
  letter-spacing: 0;
}
.changeUser{
  width: 40rpx;
  height: 44rpx;
  margin-right: 30rpx;
}
/* 报告列表样式 */
.content-warp{
  width: 694rpx;
  height: 150rpx;
  background: #FFFFFF;
  border-radius: 8rpx;
  position: relative;
  margin-top: 22rpx
}
.report-number{
  font-size: 24rpx;
  color: #A3A3A3;
  letter-spacing: 0;
  margin:30rpx 0 0 42rpx
}
.report-date{
  font-size: 30rpx;
  color: #000000;
  margin:16rpx 0 0 42rpx
}
.report-ks{
  position: absolute;
  right: 30rpx;
  top: 42rpx;
  font-size: 12px;
  color: #A3A3A3;
}
.report-xm{
  font-size: 15px;
  color: #000000;
  position: absolute;
  right: 30rpx;
  top: 80rpx;
}
.xs-left{
  width: 6rpx;
  height: 134rpx;
  position: absolute;
  left: 0rpx;
  top: 8rpx;
}
