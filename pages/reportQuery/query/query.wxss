/* 头部日期样式 */
.chooseData{
  height: 120rpx;
  width: 100%;
  background: #FFFFFF;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  font-size: 28rpx;
  color: #232323;
}
.dataTitle{
  margin-left: 28rpx;
}
.dataStart{
  margin-left: 66rpx;
  font-size: 32rpx;
  color: #2081F2;
  height: 44rpx;
  min-width: 174rpx;
}
.dataMid{
  margin-left: 60rpx;
}
.dataEnd{
  margin-left: 60rpx;
  font-size: 32rpx;
  color: #2081F2;
  height: 44rpx;
  min-width: 174rpx;
}
/* 没有查到记录 */
.noSearch-wrap{
  display: flex;
  flex-flow: column nowrap;
  justify-content: center;
  align-items: center;
}
.noSearch-wrap image{
  margin-top: 160rpx;
  width: 340rpx;
  height: 324rpx;
}
.noSearch-title{
  height: 40rpx;
  font-size: 28rpx;
  color: #666666;
  margin-top:60rpx;
  line-height: 40rpx;
}